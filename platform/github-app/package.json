{
  "name": "phoenix-project-initializer",
  "version": "1.0.0",
  "description": "GitHub App to automate project initialization from azure-project-template",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint src/",
    "test": "jest"
  },
  "dependencies": {
    "probot": "^13.0.0",
    "dotenv": "^16.3.1",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
